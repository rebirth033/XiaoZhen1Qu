<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" style="@style/ll_fb_fc_root">

    <ScrollView android:id="@+id/sv_channel" style="@style/sv_fb_fc_main">

        <LinearLayout style="@style/ll_fb_fc_main">

            <LinearLayout style="@style/ll_fb_fc_top">
                <ImageView android:id="@+id/ivBACK" style="@style/iv_fb_fc_back" android:src="@drawable/arrow_back_black" />
                <TextView android:id="@+id/tvTitle" style="@style/tv_fb_fc_title" android:text="选择发布类别" />
            </LinearLayout>

            <LinearLayout style="@style/ll_content_fb_fc_title">
                <TextView android:text="居民住宅" style="@style/tv_content_fb_fc_title" />
            </LinearLayout>

            <LinearLayout style="@style/ll_fb_fc_row">
                <LinearLayout android:id="@+id/llZZ" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivZZ" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_zz" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="整租" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="一整套房子，也包括开间" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
                <LinearLayout android:id="@+id/llHZ" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivHZ" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_hz" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="合租" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="一个房间" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
                <LinearLayout android:id="@+id/llESF" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivESF" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_esf" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="二手房" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="可用于出售的二手房产" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout style="@style/ll_content_fb_fc_title">
                <TextView android:text="商业用地" style="@style/tv_content_fb_fc_title" />
            </LinearLayout>

            <LinearLayout style="@style/ll_fb_fc_row">
                <LinearLayout android:id="@+id/llSP" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivSP" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_sp" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="商铺" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="卖场、底商、摊位、柜台等" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
                <LinearLayout android:id="@+id/llXZL" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivXZL" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_xzl" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="写字楼" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="写字楼、商务中心、商住公寓等" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
                <LinearLayout android:id="@+id/llCF" style="@style/ll_fb_fc_item">
                    <ImageView android:id="@+id/ivCF" style="@style/iv_fb_fc_item" android:src="@drawable/fb_fc_cf" />
                    <LinearLayout style="@style/ll_fb_fc_item_text">
                        <TextView style="@style/tv_fb_fc_item" android:text="厂房" />
                        <TextView style="@style/tv_fb_fc_item_remark" android:text="厂房、车位、土地、仓库等" />
                    </LinearLayout>
                    <ImageView style="@style/iv_fb_fc_forward" android:src="@drawable/arrow_forward_black" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>