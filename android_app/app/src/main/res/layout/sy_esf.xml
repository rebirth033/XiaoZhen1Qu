<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/llSY_ESF"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/sv_channel"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:fillViewport="false"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="10dp"
                android:background="#bc6ba6"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/ivBACK"
                    android:layout_width="18dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:adjustViewBounds="true"
                    android:background="#bc6ba6"
                    android:src="@drawable/arrow_back" />

                <TextView
                    android:id="@+id/etSJH"
                    android:layout_width="326dp"
                    android:layout_height="42dp"
                    android:textAlignment="center"
                    android:layout_gravity="center"
                    android:background="#bc6ba6"
                    android:textColor="#FFFFFF"
                    android:paddingLeft="5dp"
                    android:paddingTop="8dp"
                    android:text="二手房"
                    android:textSize="20dp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/llSY_ZF_ZZ"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_ZZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_ZZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="二手房"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_XF"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_XF"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_XF"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="新房"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llZP"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_HZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_HZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="商铺"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_DZ"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_DZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_DZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="写字楼"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_GRFY"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_GRFY"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_GRFY"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="厂房"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/llSY_ZF_CF"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_CF"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_CF"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="厂房"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_XZL"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_XZL"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_XZL"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="仓库"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_CW"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_CW"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_CW"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="车位"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_TD"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_TD"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/no_image" />

                    <TextView
                        android:id="@+id/tvSY_ZF_TD"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="土地"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="410dp"
                android:layout_height="48dp"
                android:background="@null"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvCNXH"
                    android:layout_width="80dp"
                    android:layout_height="48dp"
                    android:layout_marginLeft="20dp"
                    android:background="@null"
                    android:paddingTop="13dp"
                    android:text="猜你喜欢"
                    android:textSize="15sp" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCNXH"
                android:layout_width="400dp"
                android:layout_height="match_parent"
                android:background="@null"
                android:orientation="vertical">

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</LinearLayout>