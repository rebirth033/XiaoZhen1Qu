                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_SP"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/cl" />

                    <TextView
                        android:id="@+id/tvSY_ZF_SP"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="商铺"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_CW"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_CW"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/shfw" />

                    <TextView
                        android:id="@+id/tvSY_ZF_CW"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="车位"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_CF"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_CF"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/shfw" />

                    <TextView
                        android:id="@+id/tvSY_ZF_CF"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="厂房"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="410dp"
                android:layout_height="48dp"
                android:background="@null"
                android:orientation="horizontal">

                <TextView
