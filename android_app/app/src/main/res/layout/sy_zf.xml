<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/sv_channel"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:fillViewport="false"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="10dp"
                android:background="#bc6ba6"
                android:orientation="horizontal">


                <ImageView
                    android:id="@+id/ivBACK"
                    android:layout_width="18dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:adjustViewBounds="true"
                    android:background="#bc6ba6"
                    android:src="@drawable/arrow_back" />

                <EditText
                    android:id="@+id/etSJH"
                    android:layout_width="326dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    android:background="#ffffff"
                    android:paddingLeft="5dp"
                    android:hint="找房子 找工作 找服务"
                    android:textSize="18sp" />

                <ImageView
                    android:id="@+id/ivSYS"
                    android:layout_width="30dp"
                    android:layout_height="42dp"
                    android:layout_gravity="center"
                    android:adjustViewBounds="true"
                    android:background="#ffffff"
                    android:paddingRight="5dp"
                    android:src="@drawable/sys" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/llSY_ZF_ZZ"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_ZZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/fc" />

                    <TextView
                        android:id="@+id/tvSY_ZF_ZZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="整租"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llZP"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_HZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/zp" />

                    <TextView
                        android:id="@+id/tvSY_ZF_HZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="合租"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_DZ"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_DZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/jz" />

                    <TextView
                        android:id="@+id/tvSY_ZF_DZ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="短租"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_GRFY"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_GRFY"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/jz" />

                    <TextView
                        android:id="@+id/tvSY_ZF_GRFY"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="个人房源"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/llSY_ZF_XZL"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_XZL"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/es" />

                    <TextView
                        android:id="@+id/tvSY_ZF_XZL"
                        android:layout_width="match_parent"
