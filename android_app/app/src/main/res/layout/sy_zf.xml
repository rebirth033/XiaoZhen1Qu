                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_BAOJ"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_BAOJ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/cl" />

                    <TextView
                        android:id="@+id/tvSY_ZF_BAOJ"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="保洁"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_WX"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_WX"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/shfw" />

                    <TextView
                        android:id="@+id/tvSY_ZF_WX"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="维修"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llSY_ZF_PS"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivSY_ZF_PS"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/shfw" />

                    <TextView
                        android:id="@+id/tvSY_ZF_PS"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="配送"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_gravity="bottom"
        android:background="@null"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/llFB"
            android:layout_width="130dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivFB"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_fb" />

            <TextView
                android:id="@+id/tvFB"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="租房"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llXX"
            android:layout_width="130dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivXX"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_xx" />

            <TextView
                android:id="@+id/tvXX"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="发布"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llGRZX"
            android:layout_width="130dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivGRZX"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_grzx" />

            <TextView
                android:id="@+id/tvGRZX"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="我的"
                android:textAlignment="center" />

        </LinearLayout>

    </LinearLayout>

