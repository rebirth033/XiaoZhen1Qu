<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/sv_channel"
        android:layout_width="match_parent"
        android:layout_height="600dp"
        android:fillViewport="false"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:orientation="horizontal">


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivWDFB"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/wdfb" />

                    <TextView
                        android:id="@+id/tvWDFB"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="我的发布"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivWDSC"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdsc" />

                    <TextView
                        android:id="@+id/tvWDSC"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="我的收藏"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivLSJL"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/lsjl" />

                    <TextView
                        android:id="@+id/tvLSJL"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="历史记录"
                        android:textAlignment="center" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llWDDY"
                    android:layout_width="100dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/ivWDDY"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_gravity="center|bottom"
                        android:adjustViewBounds="true"
                        android:src="@drawable/wddy" />

                    <TextView
                        android:id="@+id/tvWDDY"
                        android:layout_width="match_parent"
                        android:layout_height="25dp"
                        android:text="我的订阅"
                        android:textAlignment="center" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="left"
                android:background="@null"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tvGRFW"
                    android:layout_width="match_parent"
                    android:layout_height="25dp"
                    android:paddingLeft="10dp"
                    android:paddingTop="3dp"
                    android:text="个人服务"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="400dp"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivWDZF"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdzf" />

                    <TextView
                        android:id="@+id/tvWDZF"
                        style="@style/textGRZX_GRFW"
                        android:text="我的租房" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivWDFWPJ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdpj" />

                    <TextView
                        android:id="@+id/tvWDFWPJ"
                        style="@style/textGRZX_GRFW"
                        android:text="我的评价" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="400dp"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivWDHB"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdhb" />

                    <TextView
                        android:id="@+id/tvWDHB"
                        style="@style/textGRZX_GRFW"
                        android:text="我的红包" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivWDDK"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wddk" />

                    <TextView
                        android:id="@+id/tvWDDK"
                        style="@style/textGRZX_GRFW"
                        android:text="我的贷款" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="400dp"
                android:layout_height="75dp"
                android:layout_gravity="bottom"
                android:background="@null"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivWDRZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdrz" />

                    <TextView
                        android:id="@+id/tvWDRZ"
                        style="@style/textGRZX_GRFW"
                        android:text="我的认证" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="200dp"
                    android:layout_height="75dp"
                    android:layout_gravity="left|bottom"
                    android:background="@null"
                    android:orientation="horizontal">

                    <ImageView
                            android:id="@+id/ivWDQZ"
                        android:layout_width="27dp"
                        android:layout_height="27dp"
                        android:layout_marginLeft="30dp"
                        android:layout_gravity="center|top"
                        android:adjustViewBounds="true"
                        android:src="@drawable/grzx_wdqz" />

                    <TextView
                        android:id="@+id/tvWDQZ"
                        style="@style/textGRZX_GRFW"
                        android:text="我的求职" />

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:layout_gravity="bottom"
        android:background="@null"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/llSY"
            android:layout_width="100dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivSY"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_sy" />

            <TextView
                android:id="@+id/tvSY"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="首页"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llFB"
            android:layout_width="100dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivFB"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_fb" />

            <TextView
                android:id="@+id/tvFB"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="发布"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llXX"
            android:layout_width="100dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivXX"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_xx" />

            <TextView
                android:id="@+id/tvXX"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="消息"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/llGRZX"
            android:layout_width="100dp"
            android:layout_height="75dp"
            android:layout_gravity="left|bottom"
            android:background="@null"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/ivGRZX"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_gravity="center|bottom"
                android:adjustViewBounds="true"
                android:src="@drawable/dbcd_grzx" />

            <TextView
                android:id="@+id/tvGRZX"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:text="个人中心"
                android:textAlignment="center" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>
