<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ll_fb_zxjc_root">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="sv_fb_zxjc_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">600dp</item>
        <item name="android:fillViewport">false</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="ll_fb_zxjc_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="ll_fb_zxjc_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="iv_fb_zxjc_back">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="tv_fb_zxjc_title">
        <item name="android:layout_width">330dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ll_fb_zxjc_row">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">top</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="ll_fb_zxjc_item_first">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:background">@drawable/border</item>
    </style>
    <style name="ll_fb_zxjc_item">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:background">@drawable/border</item>
    </style>
    <style name="tv_fb_zxjc_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#000000</item>
    </style>
</resources>
