<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ll_fb_fc_root">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="sv_fb_fc_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">600dp</item>
        <item name="android:fillViewport">false</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="ll_fb_fc_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="ll_fb_fc_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="iv_fb_fc_back">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="tv_fb_fc_title">
        <item name="android:layout_width">330dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ll_content_fb_fc_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@color/colorGray</item>
    </style>
    <style name="tv_content_fb_fc_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">8dp</item>
        <!--<item name="android:layout_marginBottom">1dp</item>-->
        <item name="android:background">@color/colorWhite</item>
    </style>
    <style name="tv_fb_fc_wdfb">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="ll_fb_fc_row">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">top</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>
    <style name="ll_fb_fc_item">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:background">@drawable/border</item>
    </style>
    <style name="iv_fb_fc_item">
        <item name="android:layout_width">38dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:layout_gravity">center|center</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="ll_fb_fc_item_text">
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">10dp</item>
    </style>
    <style name="tv_fb_fc_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="tv_fb_fc_item_remark">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textAlignment">textStart</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>
    <style name="iv_fb_fc_forward">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_gravity">center|center</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
</resources>
