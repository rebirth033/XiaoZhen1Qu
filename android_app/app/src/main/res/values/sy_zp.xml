<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ll_sy_zp_root">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="sv_sy_zp_channel">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">600dp</item>
        <item name="android:fillViewport">false</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="ll_sy_zp_main">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="ll_sy_zp_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:background">#bc6ba6</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="iv_sy_zp_back">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:background">#bc6ba6</item>
    </style>
    <style name="tv_sy_zp_title">
        <item name="android:layout_width">326dp</item>
        <item name="android:layout_height">42dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">#bc6ba6</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:textSize">20dp</item>
    </style>
    <style name="ll_sy_zp_fl_row">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="ll_sy_zp_fl_item">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:layout_gravity">left|bottom</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="iv_sy_zp_fl_item">
        <item name="android:layout_width">38dp</item>
        <item name="android:layout_height">38dp</item>
        <item name="android:layout_gravity">center|bottom</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="tv_sy_zp_fl_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="ll_sy_zp_cnxh_top">
        <item name="android:layout_width">410dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="tv_sy_zp_cnxh_top">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:paddingTop">13dp</item>
        <item name="android:textSize">15dp</item>
    </style>
    <style name="ll_sy_zp_cnxh_content">
        <item name="android:layout_width">400dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
</resources>
