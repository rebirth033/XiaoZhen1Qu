@{
    ViewBag.Title = "PFCG_KQ";
    Layout = "~/Areas/Business/Views/Shared/HTFB.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="@Url.Content("~/Areas/Business/Css/HTFB/PFCG/PFCG_KQ.css")" rel="stylesheet" />
    <script type="text/javascript" src="@Url.Content("~/Areas/Business/Scripts/Business/HTFB/PFCG/PFCG_KQ.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Areas/Business/Scripts/Business/HTFB/PFCG/PFCG_KQYZ.js")"></script>
</head>
<body>
    <div>
        <div id="divTop" class="div_top" style="z-index: 1002; position:relative;">
