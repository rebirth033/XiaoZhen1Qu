@{
    ViewBag.Title = "YHDLXX";
    Layout = "~/Areas/Business/Views/Shared/_ListLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <script type="text/javascript" src="@Url.Content("~/Areas/Business/Scripts/Business/YHDLXX.js")"></script>
</head>
<body>
    <div>
        <table class="table" style="width: 300px;">
            <tr>
                <td class="headtd"><span class="title">欢迎来到小镇1区</span></td>
            </tr>
            <tr>
                <td>
                    <ul id="myTab" class="nav nav-tabs" style="width: 300px">
                        <li style="width: 150px"><a href="#username" data-toggle="tab">用户名密码登录</a></li>
                        <li style="width: 150px;"><a href="#cellphone" data-toggle="tab">手机验证码登录</a></li>
                    </ul>
                </td>
            </tr>
        </table>
    </div>
    <div id="myTabContent" class="tab-content">
        <div id="cellphone" class="tab-pane fade in active">
            <table class="table" style="width: 600px;">
                <tr>
                    <td id="tdSJ">
                        <input type="text" class="form-control input_text" id="SJ" column="SJ" placeholder="请输入手机号">
                        <input type="button" class="btn btn-info" id="btnHQYZM" value="获取验证码" />
                    </td>
                    <td class="Info" id="SJInfo" style="width: 50%"></td>
                </tr>
                <tr>
                    <td><input type="text" class="form-control input_text" id="YZM" column="YZM" placeholder="请输入验证码"></td>
                    <td class="Info" id="YZMInfo" style="width: 50%"></td>
                </tr>
                <tr>
                    <td style="text-align: left"><input type="checkbox" /><span style="vertical-align: top; margin-left: 10px; font-size: 14px; color: #999;">下次自动登录</span></td>
                </tr>
                <tr>
                    <td><input type="button" class="btn btn-info input_text" id="DL" value="登录" /></td>
                </tr>
            </table>
        </div>
        <div id="username" class="tab-pane fade">
            <table class="table" style="width: 600px;">
                <tr>
                    <td style="width: 50%"><input type="text" class="form-control input_text" id="YHM" column="YHM" placeholder="请输入用户名"></td>
                    <td class="Info" id="YHMInfo" style="width: 50%"></td>
                </tr>
                <tr>
                    <td>
                        <input type="password" class="form-control input_text" id="MM" column="MM" placeholder="请输入登录密码">
                    </td>
                    <td class="Info" id="MMInfo" style="width: 50%"></td>
                </tr>
                <tr>
                    <td><input type="button" class="btn btn-info input_text" id="DL" value="登录" /></td>
                </tr>
            </table>
        </div>
    </div>
</body>
</html>
