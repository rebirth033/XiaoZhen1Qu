@{
    ViewBag.Title = "信息小镇_房产查询";
    Layout = "~/Areas/Business/Views/Shared/QTCXLB.cshtml";
}
@section Section_Head {
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <link href="@Url.Content("~/Areas/Business/Css/CXLB/FCCX.css")" rel="stylesheet" />
    <script type="text/javascript" src="@Url.Content("~/Areas/Business/Scripts/Business/CXLB/FCCX/FCCX_ZZF.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Areas/Business/Scripts/Business/Common/XZQ.js")"></script>
}
@section Section_Body {
    <div id="divHead" class="div_head">
        <div class="div_head_left">
            <img class="img_head_left_logo" src="@Url.Content("~/Areas/Business/Css/images/logo_blue.png")" />
        </div>
        <div class="div_head_right">
            <div class="div_head_right_ss">
                <span id="spanXSLC" class="span_spaninput_out SRK">
                    <input class="span_spaninput_in" id="SS" type="text" placeholder="找房子,找工作,找服务," />
                    <span class="span_ss">搜索</span>
                </span>
                <span class="span_head_right_ss">租房子</span>
                <span class="span_head_right_ss">二手车</span>
                <span class="span_head_right_ss">兼职</span>
                <span class="span_head_right_ss">装修服务</span>
                <span class="span_head_right_ss">旅游度假</span>
                <span class="span_head_right_ss">居民搬家</span>
                <span class="span_head_right_ss">演出表演</span>
                <span class="span_head_right_ss">运动健身</span>
            </div>
            <span class="span_fbxx" id="span_fbxx">免费发布信息</span>
        </div>
    </div>
    <div id="divNav" class="div_nav">
        <ul class="ul_nav">
            <li class="li_nav_font">信息小镇</li>
            <li class="li_nav_split">></li>
            <li class="li_nav_font">@ViewData["XZQ"]房产信息</li>
            <li class="li_nav_split">></li>
            <li class="li_nav_font">@ViewData["XZQ"]整租房</li>
        </ul>
    </div>
    <div id="divCondition" class="div_condition">
        <ul class="ul_condition_head">
            <li id="li_condition_head_qyzf" class="li_condition_head">区域找房</li>
            <li id="li_condition_head_dtzf" class="li_condition_head">地铁找房</li>
            <li id="li_condition_head_xqzf" class="li_condition_head">小区找房</li>
        </ul>
    </div>
    <div id="divConditionSelect" class="div_condition_select">
        <ul class="ul_condition_select">
            <li class="li_condition_select_first">筛选条件</li>
            <li class="li_condition_select"><span>仓山</span><em>x</em></li>
            <li class="li_condition_select"><span>500-1000元</span><em>x</em></li>
            <li class="li_condition_select"><span>整套出租</span><em>x</em></li>
        </ul>
    </div>
    <div id="divBody" class="div_body">
        <ul class="ul_body_head">
            <li class="li_body_head">@ViewData["XZQ"]出租</li>
            <li class="li_body_head">个人房源</li>
            <li class="li_body_head">经纪人</li>
            <li class="li_body_head">热租房源</li>
            <li class="li_body_head">品牌公寓</li>
        </ul>
        <div id="div_body_left" class="div_body_left">
            <ul id="ul_body_left" class="ul_body_left">
                <li class="li_body_left">
                    <div class="div_li_body_left_left">
                        <img class="img_li_body_left" src="@Url.Content("~/Areas/Business/Css/images/test.jpg")"/>
                    </div>
                    <div class="div_li_body_left_center">
                        <p class="p_li_body_left_center_bt">台江万达对面鳌峰小区精装大三房出租，房东自住装修</p>
                        <p class="p_li_body_left_center_cs">整套出租 / 1室1厅1卫 / 42平米 / 精装修 / 南北 / 低层[共30层]</p>
                        <p class="p_li_body_left_center_dz">金诺大厦 [鼓楼-温泉-华林路281号] 11月2日</p>
                    </div>
                </li>
            </ul>
            <div class="div_main_info_bottom">
                <div class="div_main_info_bottom_fy" id="div_main_info_bottom_fy">
                </div>
            </div>
        </div>
        <div class="div_body_right"></div>
    </div>
}