                <div id="div_xxbsc">
                    <table class="table_wtjj">
                        <tr class="tr_wtjj">
                            <td class="td_wtjj_inner_left">请输入您的信息编号：</td>
                            <td class="td_wtjj_inner_right">
                                <input type="text" id="inputXSJ" class="input_content" placeholder="请输入信息编号">
